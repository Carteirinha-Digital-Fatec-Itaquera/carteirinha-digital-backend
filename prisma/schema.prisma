// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client"
  output = "./generated/prisma"
  // previewFeatures = ["relationJoins", "driverAdapters", "metrics", "queryCompiler"]
  moduleFormat = "cjs"
}

datasource db {
  provider = "mysql"
}

model Student {
  ra        String @id @unique
  course    String
  period    String
  status    String
  name      String
  admission String
  email     String @unique
  cpf       String @unique
  rg        String @unique
  qrcode    String?
  photo     String?
  birthDate String
  dueDate   String
  password  String?
}
