generator client {
  provider = "prisma-client-js"
  moduleFormat= "cjs"
}

datasource db {
  provider = "postgresql"
}

model Student {
  ra        String   @id
  course    String
  period    String
  status    String
  name      String
  admission String
  email     String   @unique
  cpf       String   @unique
  rg        String   @unique
  qrcode    String?
  photo     String?
  birthDate DateTime
  dueDate   DateTime
  password  String
}

model Secretary {
  id Int @id @default(autoincrement())
  name String 
  email String @unique
  dueDate DateTime
  password String
}